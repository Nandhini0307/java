pipeline {
    agent any
    tools {
                  maven "apache-maven-3.9.8"
    }
    stages {
        stage('Build') {
            steps {
               git url:'https://github.com/Nandhini0307/java.git', branch: 'main'
               bat "mvn -Dmaven.test.failure.ignore=true clean package"
            }
            post {
                     success {
                archiveArtifacts 'target/*.jar'
                }
            }}
     }}

